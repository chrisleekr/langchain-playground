{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/*.ts"],
  "project": ["src/**/*.ts"],
  "ignore": ["**/__tests__/**", "**/*.test.ts", "**/*.spec.ts"],
  "ignoreDependencies": ["@chroma-core/default-embed", "pino-pretty", "tsc-alias"],
  "ignoreBinaries": ["semantic-release", "ngrok"],
  "ignoreExportsUsedInFile": true
}
