{
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {},
  "files.insertFinalNewline": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "search.exclude": {
    ".git": true,
    ".build": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/coverage": true,
    "**/node_modules": true
  },
  "cSpell.words": [
    "anonymised",
    "bbands",
    "Bollinger",
    "buildx",
    "chrisleekr",
    "chromadb",
    "commitlint",
    "cummulative",
    "deepseek",
    "distill",
    "docstore",
    "Groq",
    "healthcheck",
    "hgetall",
    "langchain",
    "langgraph",
    "llms",
    "localai",
    "MACD",
    "mrkdwn",
    "mxbai",
    "Ollama",
    "openai",
    "openweb",
    "pino",
    "redlock",
    "tradingview",
    "tulind",
    "typebox",
    "typecheck",
    "uuidv",
    "vectorstore",
    "vectorstores"
  ],
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.mode": "all",
  "cSpell.language": "en-AU"
}
