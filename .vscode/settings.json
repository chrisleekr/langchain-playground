{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {},
  "files.insertFinalNewline": true,
  "search.exclude": {
    ".git": true,
    ".build": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/coverage": true,
    "**/node_modules": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "js/ts.implicitProjectConfig.checkJs": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[sql]": {
    "editor.defaultFormatter": "bradymholt.pgformatter"
  },
  "cSpell.language": "en-US",
  "cSpell.words": [
    "analyzer",
    "anonymised",
    "bbands",
    "Bollinger",
    "buildkitd",
    "buildx",
    "CERTDIR",
    "chrisleekr",
    "chromadb",
    "Chunker",
    "commitlint",
    "cummulative",
    "debugmeta",
    "deepseek",
    "dind",
    "distill",
    "docstore",
    "fastify",
    "Groq",
    "Guids",
    "healthcheck",
    "hgetall",
    "interruptible",
    "labeler",
    "langchain",
    "langgraph",
    "llms",
    "localai",
    "localize",
    "MACD",
    "Millis",
    "minimize",
    "minimizer",
    "modelcontextprotocol",
    "mpim",
    "mrkdwn",
    "mxbai",
    "NEWRELIC",
    "nrql",
    "Ollama",
    "onnxruntime",
    "openai",
    "openweb",
    "optimization",
    "pinecone",
    "pino",
    "qdrant",
    "qwen",
    "redlock",
    "robwilkes",
    "rspack",
    "slackify",
    "srvaroa",
    "substate",
    "substatus",
    "summarise",
    "TOOLSETS",
    "tradingview",
    "tulind",
    "typebox",
    "typecheck",
    "uuidv",
    "vectorstore",
    "vectorstores",
    "WEBUI"
  ],
  "eslint.format.enable": true,
  "eslint.codeActionsOnSave.mode": "all"
}
