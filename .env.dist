NODE_ENV=development

# Server mode - fastify or slack
SERVER_MODE=fastify

PORT=8080
HOST=0.0.0.0

TIMEZONE='Australia/Melbourne'

# CORS Settings
CORS_ORIGIN="*" # Allowed CORS origin, adjust as necessary

# RateLimit Settings
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Redis
REDIS_URL=redis://redis:6379

# Ollama
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_MODEL=mistral-small3.1:24b-instruct-2503-q4_K_M # Worked well
OLLAMA_EMBEDDING_MODEL=mxbai-embed-large:latest

# OpenAI
OPENAI_BASE_URL=
OPENAI_API_KEY=SOMETHING
OPENAI_TEMPERATURE=0
OPENAI_MODEL=gpt-5.2

# GROQ - https://console.groq.com/
GROQ_API_KEY=SOMETHING
GROQ_TEMPERATURE=0
GROQ_MODEL=qwen3-32b

# Unstructured API
UNSTRUCTURED_API_URL=http://unstructured-api:8000/general/v0/general

# Chroma Vector Store
CHROMA_HOST=http://chroma:8000

# Qdrant Vector Store
QDRANT_URL=http://qdrant:6333

# LangChain LangSmith - https://smith.langchain.com/
LANGCHAIN_PROJECT=default
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=SOMETHING

# Pinecone.io
PINECONE_API_KEY=SOMETHING

# Slack - https://api.slack.com/apps
SLACK_BOT_TOKEN=SOMETHING
SLACK_SIGNING_SECRET=SOMETHING

# MCP
MCP_GITHUB_PERSONAL_ACCESS_TOKEN=SOMETHING
MCP_BRAVE_API_KEY==SOMETHING

# Document
DOCUMENT_COLLECTION_NAME=
DOCUMENT_CONFLUENCE_BASE_URL=
DOCUMENT_CONFLUENCE_SPACE_KEY=
DOCUMENT_CONFLUENCE_ACCESS_TOKEN=
DOCUMENT_CONFLUENCE_MAX_PAGES=
DOCUMENT_CONFLUENCE_UPDATE_INTERVAL=

# AWS
AWS_BEDROCK_REGION=us-east-1
AWS_BEDROCK_MODEL=us.anthropic.claude-opus-4-5-20251101-v1:0
AWS_BEDROCK_EMBEDDING_MODEL=amazon.titan-embed-text-v2:0
AWS_BEDROCK_KNOWLEDGE_BASE_ID=
AWS_BEDROCK_PROFILE=default
AWS_BEDROCK_ACCESS_KEY_ID=
AWS_BEDROCK_SECRET_ACCESS_KEY=

AWS_OPENSEARCH_REGION=us-east-1
AWS_OPENSEARCH_ENDPOINT=
AWS_OPENSEARCH_INDEX=

# NewRelic
NEWRELIC_ACCOUNT_ID=
NEWRELIC_API_KEY=
NEWRELIC_ENVOY_LOGS_EXCLUDE_SERVICES='["unrelated-service-name"]'
NEWRELIC_URL_LOGS_PROPERTIES='["url-related-log-property"]'
NEWRELIC_GET_RELEVANT_URLS_ADDITIONAL_INSTRUCTIONS="- URL Construction Rules:\n  - If the field key 'url' is present, convert to 'http://my-url.com/<value>' replacing <value> with the field value"

# Sentry
SENTRY_ORGANIZATION_SLUG=
SENTRY_AUTH_TOKEN=

# GitHub
GITHUB_PERSONAL_ACCESS_TOKEN=
GITHUB_OWNER=
