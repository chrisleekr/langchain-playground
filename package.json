{
  "name": "langchain-playground",
  "version": "0.0.1",
  "description": "A LangChain playground using TypeScript",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "postinstall": "npm run lockfile:validate",
    "lockfile:validate": "npm ls --omit=optional > /dev/null || echo 'Lock file validation complete'",
    "lockfile:clean": "rm -rf node_modules package-lock.json && HUSKY=0 npm install",
    "start": "node dist/index.js",
    "dev": "nodemon -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
    "dev:rspack": "concurrently \"rspack build --watch\" \"nodemon -r dotenv/config dist/index.js\"",
    "dev:watch": "nodemon",
    "dev:script": "ts-node -r dotenv/config -r tsconfig-paths/register",
    "dev:ngrok": "ngrok http 8080",
    "build": "rspack build",
    "clean": "rm -rf ./dist/",
    "lint": "eslint ./",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --config .prettierrc.js .",
    "format:fix": "prettier --config .prettierrc.js --write .",
    "test": "jest --coverage --logHeapUsage --detectOpenHandles",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build --progress=plain -t chrisleekr/langchain-playground:local .",
    "docker:run": "docker run --env-file .env -p 8080:8080 --name langchain-playground chrisleekr/langchain-playground:local"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisleekr/langchain-playground.git"
  },
  "keywords": [
    "LangChain",
    "TypeScript"
  ],
  "author": "Chris Lee <git@chrislee.kr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisleekr/langchain-playground/issues"
  },
  "homepage": "https://github.com/chrisleekr/langchain-playground#readme",
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-provider-node": "^3.901.0",
    "@aws-sdk/credential-providers": "^3.901.0",
    "@aws-sdk/types": "^3.901.0",
    "@chroma-core/default-embed": "0.1.8",
    "@fastify/compress": "8.1.0",
    "@fastify/cors": "11.1.0",
    "@fastify/helmet": "13.0.2",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/type-provider-typebox": "6.0.0",
    "@langchain/aws": "^0.1.15",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.78",
    "@langchain/groq": "0.2.4",
    "@langchain/langgraph": "0.4.9",
    "@langchain/mcp-adapters": "0.6.0",
    "@langchain/ollama": "0.2.4",
    "@langchain/openai": "0.6.14",
    "@langchain/pinecone": "0.2.0",
    "@langchain/qdrant": "0.1.3",
    "@langchain/redis": "0.1.3",
    "@opensearch-project/opensearch": "^3.5.1",
    "@qdrant/js-client-rest": "1.15.1",
    "@sinclair/typebox": "0.34.41",
    "@slack/bolt": "4.4.0",
    "aws-opensearch-connector": "^1.2.0",
    "chromadb": "3.0.17",
    "config": "4.1.1",
    "dayjs": "1.11.19",
    "dotenv": "16.6.1",
    "fastify": "5.6.1",
    "html-to-text": "9.0.5",
    "http-status-codes": "2.3.0",
    "ioredis": "5.8.0",
    "langchain": "0.3.35",
    "pdf-parse": "1.1.1",
    "pino": "9.13.0",
    "semantic-chunking": "2.4.4",
    "slackify-markdown": "4.5.0",
    "typescript": "5.9.3",
    "uuid": "13.0.0",
    "yaml": "2.8.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@jest/globals": "30.2.0",
    "@rspack/cli": "1.5.8",
    "@rspack/core": "1.5.8",
    "@slack/types": "2.16.0",
    "@types/config": "3.3.5",
    "@types/jest": "30.0.0",
    "@types/node": "24.6.2",
    "@types/pino": "7.0.5",
    "@types/uuid": "11.0.0",
    "@typescript-eslint/eslint-plugin": "8.45.0",
    "@typescript-eslint/parser": "8.45.0",
    "branch-name-lint": "3.0.1",
    "concurrently": "9.2.1",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "lint-staged": "16.2.3",
    "nodemon": "3.1.10",
    "pino-pretty": "13.1.1",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0"
  },
  "lint-staged": {
    "**/*": "npm run format:fix",
    "**/*.{js,ts}": "npm run lint:fix"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "volta": {
    "node": ">=22.17.0",
    "npm": ">=11.5.2"
  },
  "overrides": {
    "ignore": "5.3.2"
  }
}
