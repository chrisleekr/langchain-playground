{
  "name": "langchain-playground",
  "version": "0.0.1",
  "description": "A LangChain playground using TypeScript",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "postinstall": "npm run lockfile:validate",
    "lockfile:validate": "npm ls --omit=optional > /dev/null || echo 'Lock file validation complete'",
    "lockfile:clean": "rm -rf node_modules package-lock.json && HUSKY=0 npm install",
    "start": "node dist/index.js",
    "dev": "nodemon -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
    "dev:rspack": "concurrently \"rspack build --watch\" \"nodemon -r dotenv/config dist/index.js\"",
    "dev:watch": "nodemon -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
    "dev:script": "ts-node -r dotenv/config -r tsconfig-paths/register",
    "dev:ngrok": "ngrok http 8080",
    "build": "rspack build",
    "clean": "rm -rf ./dist/",
    "lint": "eslint --cache --cache-location .eslintcache ./",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --config .prettierrc.js .",
    "format:fix": "prettier --config .prettierrc.js --write .",
    "test": "jest --coverage --logHeapUsage --detectOpenHandles --maxWorkers=50%",
    "typecheck": "tsc --noEmit",
    "ci": "npm run typecheck && npm run lint && npm test && npm run build",
    "knip": "knip",
    "docker:build": "docker build --progress=plain -t chrisleekr/langchain-playground:local .",
    "docker:run": "docker run --env-file .env -p 8080:8080 --name langchain-playground chrisleekr/langchain-playground:local"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisleekr/langchain-playground.git"
  },
  "keywords": [
    "LangChain",
    "TypeScript"
  ],
  "author": "Chris Lee <git@chrislee.kr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisleekr/langchain-playground/issues"
  },
  "homepage": "https://github.com/chrisleekr/langchain-playground#readme",
  "dependencies": {
    "@aws-crypto/sha256-js": "5.2.0",
    "@aws-sdk/client-cloudwatch": "3.975.0",
    "@aws-sdk/client-cloudwatch-logs": "3.980.0",
    "@aws-sdk/client-ecs": "3.980.0",
    "@aws-sdk/client-pi": "3.980.0",
    "@aws-sdk/client-rds": "3.975.0",
    "@aws-sdk/credential-provider-node": "3.972.4",
    "@aws-sdk/credential-providers": "3.980.0",
    "@aws-sdk/types": "3.973.1",
    "@chroma-core/default-embed": "0.1.9",
    "@fastify/compress": "8.3.1",
    "@fastify/cors": "11.2.0",
    "@fastify/helmet": "13.0.2",
    "@fastify/rate-limit": "10.3.0",
    "@fastify/type-provider-typebox": "6.1.0",
    "@langchain/aws": "1.2.2",
    "@langchain/classic": "1.0.13",
    "@langchain/community": "1.1.10",
    "@langchain/core": "1.1.18",
    "@langchain/groq": "1.0.4",
    "@langchain/langgraph": "1.1.2",
    "@langchain/langgraph-supervisor": "1.0.1",
    "@langchain/mcp-adapters": "1.1.2",
    "@langchain/ollama": "1.2.2",
    "@langchain/openai": "1.2.4",
    "@langchain/pinecone": "1.0.1",
    "@langchain/qdrant": "1.0.1",
    "@langchain/redis": "1.0.1",
    "@langchain/textsplitters": "1.0.1",
    "@opensearch-project/opensearch": "3.5.1",
    "@sinclair/typebox": "0.34.48",
    "@slack/bolt": "4.6.0",
    "@slack/web-api": "7.13.0",
    "@toon-format/toon": "2.1.0",
    "aws-opensearch-connector": "1.2.0",
    "chromadb": "3.2.2",
    "config": "4.2.0",
    "dayjs": "1.11.19",
    "decimal.js": "10.6.0",
    "dotenv": "16.6.1",
    "fastify": "5.7.2",
    "gpt-tokenizer": "3.4.0",
    "html-to-text": "9.0.5",
    "http-status-codes": "2.3.0",
    "ioredis": "5.9.2",
    "node-cron": "4.2.1",
    "pdf-parse": "1.1.1",
    "pino": "10.3.0",
    "simple-git": "3.30.0",
    "slackify-markdown": "4.5.0",
    "typescript": "5.9.3",
    "uuid": "13.0.0",
    "yaml": "2.8.2",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@babel/core": "7.29.0",
    "@babel/preset-env": "7.29.0",
    "@commitlint/cli": "20.4.0",
    "@commitlint/config-conventional": "20.4.0",
    "@jest/globals": "30.2.0",
    "@rspack/cli": "1.7.4",
    "@rspack/core": "1.7.4",
    "@slack/types": "2.19.0",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@types/config": "3.3.5",
    "@types/jest": "30.0.0",
    "@types/node": "25.0.10",
    "@types/node-cron": "3.0.11",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "babel-jest": "30.2.0",
    "branch-name-lint": "3.0.1",
    "concurrently": "9.2.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.12.1",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-security": "3.0.1",
    "husky": "9.1.7",
    "ignore-loader": "0.1.2",
    "jest": "30.2.0",
    "knip": "5.82.1",
    "lint-staged": "16.2.7",
    "nodemon": "3.1.11",
    "pino-pretty": "13.1.3",
    "prettier": "3.8.1",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.16",
    "tsconfig-paths": "4.2.0"
  },
  "lint-staged": {
    "**/*": "npm run format:fix",
    "**/*.{js,ts}": "npm run lint:fix"
  },
  "engines": {
    "node": ">=22.17.0"
  },
  "volta": {
    "node": "22.17.0",
    "npm": "11.5.2"
  },
  "overrides": {
    "ignore": "5.3.2"
  }
}
